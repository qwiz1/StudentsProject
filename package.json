{
  "scripts": {
    "start:dev": "nodemon ./src/server.ts",
    "build": "tsc",
    "migrate:create": "ts-node node_modules/.bin/node-pg-migrate create -j ts -m src/migrations",
    "migrate:up": "ts-node node_modules/.bin/node-pg-migrate up -j ts -m src/migrations",
    "migrate:down": "ts-node node_modules/.bin/node-pg-migrate down -j ts -m src/migrations"
  },
  "devDependencies": {
    "@types/express": "4.17.15",
    "@types/ws": "8.5.4",
    "nodemon": "2.0.20",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "dependencies": {
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "node-pg-migrate": "6.2.2",
    "pg": "8.8.0",
    "ws": "8.12.0"
  }
}
